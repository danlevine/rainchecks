{
  "name": "rainchecks",
  "version": "1.0.0",
  "description": "A simple web app to save good ideas for later.",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "build": "rm -rf ./dist && cross-env NODE_ENV=production webpack -p --progress && cp index.html dist/",
    "deploy": "rm -rf ./dist && cross-env NODE_ENV=production webpack -p --progress && cp index.html dist/ && firebase deploy"
